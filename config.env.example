# GCP / GKE
PROJECT_ID=tici-dev
REGION=asia-east2
CLUSTER_NAME=tici-test

# Optional: override default VPC
NETWORK=default
SUBNETWORK=default

# Disable API management if you don't have Service Usage permissions
# MANAGE_APIS=false

# Namespace & storage class
NAMESPACE=tidb-fts
STORAGE_CLASS=premium-rwo

# Storage mode: gcs or minio
STORAGE_MODE=gcs

# Optional: for MinIO mode (if STORAGE_MODE=minio)
# S3_BUCKET=tici
# S3_ACCESS_KEY=change_me
# S3_SECRET_KEY=change_me

# Optional: bucket location (defaults to REGION)
# BUCKET_LOCATION=asia-east2

# Replicas (defaults all to 1)
# PD_REPLICAS=1
# TIDB_REPLICAS=1
# TIDB_WORKER_REPLICAS=1
# TIKV_REPLICAS=1
# TIKV_WORKER_REPLICAS=1
# TIFLASH_CN_REPLICAS=1
# TICDC_REPLICAS=1
# TICI_META_REPLICAS=1
# TICI_WORKER_REPLICAS=1
# MINIO_REPLICAS=1

# Image overrides (optional)
# TIDB_BASE_IMAGE=
# TIDB_VERSION=
# TIKV_BASE_IMAGE=
# TIKV_VERSION=
# PD_BASE_IMAGE=
# PD_VERSION=
# TIKV_WORKER_IMAGE=
# TIFLASH_IMAGE=
# TICDC_IMAGE=
# TICI_IMAGE=
# TIDB_OPERATOR_IMAGE=
# TIDB_DISCOVERY_IMAGE=
# BUSYBOX_IMAGE=
# TIDB_HELPER_IMAGE=
# MINIO_IMAGE=
# MYSQL_CLIENT_IMAGE=

# Changefeed
# CHANGEFEED_ID=tici
